{
  "name": "audilex",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/lab": "^6.0.0-beta.9",
    "@mui/material": "^6.1.0",
    "@reduxjs/toolkit": "^2.2.7",
    "ag-grid-community": "^32.1.0",
    "ag-grid-react": "^32.1.0",
    "axios": "^1.7.7",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.14.1",
    "web-vitals": "^4.2.3",
    "yup": "^1.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "format": "prettier --write ./src",
    "lint": "eslint src --ext .js,.jsx --fix",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:dev_ec": "env-cmd -f .env.ec.admin_dev react-scripts build",
    "deploy:dev_ec": "aws s3 sync build/ s3://ocean-frontend-dev --acl public-read --profile experts --delete",
    "deploy_url:dev_ec": "echo '🌐 http://ocean-frontend-dev.s3-website-us-east-1.amazonaws.com'",
    "bd_admin_dev": "yarn build:dev_ec && yarn deploy:dev_ec && yarn deploy_url:dev_ec",
    "build:dev_ec_2": "env-cmd -f .env.ec.dev_2 react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "prettier": "^3.0.3",
    "yarn": "^1.22.19"
  }
}
